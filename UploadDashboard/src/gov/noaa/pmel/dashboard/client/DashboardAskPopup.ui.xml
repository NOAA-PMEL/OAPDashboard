<!-- DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent" -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="FixedWidth.css" />

	<g:VerticalPanel addStyleNames="{style.infopanel}" > <!-- horizontalAlignment="ALIGN_CENTER"> -->
        <g:FlowPanel addStyleNames="{style.horizontalpanel}" >
            <g:Image addStyleNames="{style.askDialogIcon}" ui:field="askTypeIcon"/>
    		<g:HTML addStyleNames="{style.infohtml}" ui:field="askHtml" />
        </g:FlowPanel>
		<g:FlowPanel addStyleNames="{style.horizontalpanel}">
			<g:Button addStyleNames="{style.adjustingbutton}" ui:field="yesButton" />
			<g:Button addStyleNames="{style.adjustingbutton}" ui:field="noButton" />
		</g:FlowPanel>
	</g:VerticalPanel>

</ui:UiBinder>
